{
  "$schema": "http://json.schemastore.org/template",
  "identity": "AxelKjonsberg.AzureUserFlow.Templates.ApiConnector",
  "name": "Azure Functions (isolated) – B2C API Connector",
  "shortName": "b2c-apiconnector",
  "sourceName": "ApiConnector",
  "author": "Axel M. Kjønsberg",
  "classifications": [ "Azure", "Functions", "B2C" ],
  "preferNameDirectory": true,
  "tags": {
    "type": "project",
    "language": "C#"
  },
  "symbols": {
    "UserFlowHelpersPackageId": {
      "type": "parameter",
      "datatype": "text",
      "replaces": "[[UserFlowHelpersPackageId]]",
      "defaultValue": "AxelKjonsberg.AzureUserFlow.Helpers"
    },
    "UserFlowHelpersVersion": {
      "type": "parameter",
      "datatype": "text",
      "replaces": "[[UserFlowHelpersVersion]]",
      "defaultValue": "0.1.0-beta.1"
    }
  },
  "primaryOutputs": [
    { "path": "src/ApiConnector.csproj" }
  ]
}
